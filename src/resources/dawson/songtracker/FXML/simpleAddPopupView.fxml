<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<fx:root type="Pane" xmlns="http://javafx.com/javafx"
         xmlns:fx="http://javafx.com/fxml"
         layoutX="65.0" layoutY="53.0" prefHeight="274.0" prefWidth="518.0" style="-fx-background-color: white; -fx-border-style: solid;">
    <children>
        <VBox alignment="CENTER_LEFT" layoutX="14.0" layoutY="14.0">
            <children>
                <Label fx:id="title" text="Add recording">
                    <font>
                        <Font size="20.0" />
                    </font>
                </Label>
            </children>
        </VBox>
        <Button layoutX="195.0" layoutY="202.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="100.0" text="Add" onMouseClicked="#onAdd"/>
        <Button layoutX="496.0" layoutY="4.0" minHeight="5.0" minWidth="5.0" mnemonicParsing="false" onAction="#hide" prefHeight="15.0" prefWidth="15.0" style="-fx-background-color: red; -fx-background-radius: 100%;" />
        <Label layoutX="33.0" layoutY="129.0" text="Name" />
        <TextField fx:id="textInput" layoutX="91.0" layoutY="125.0" prefHeight="25.0" prefWidth="386.0"/>
    </children>
</fx:root>
