         prefHeight="400.0" prefWidth="600.0" type="Pane"
         style="-fx-background-color: white; -fx-border-style: solid;">

    <children>

        <HBox layoutY="51.0" prefHeight="223.0" prefWidth="518.0">
            <children>
                <VBox fx:id="leftCol" prefHeight="197.0" prefWidth="492.0">
                    <children>
                        <HBox alignment="CENTER_LEFT" spacing="50.0">
                            <VBox.margin>
                                <Insets left="20.0" />
                            </VBox.margin>
                        </HBox>
                    </children>
                </VBox>
                <VBox fx:id="rightCol" alignment="TOP_CENTER" layoutX="10.0" layoutY="10.0" prefHeight="197.0" prefWidth="492.0">
                    <children>
                        <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="171.0" spacing="50.0">
                        </HBox>
                    </children>
                </VBox>
            </children>
        </HBox>
        <Button text="Update" onMouseClicked="#onUpdate"/>

    </children>
</fx:root>
